<div class="row">      <div class="col-md-12" style="padding-left: 50px;">            <div class="col-md-10 col-md-offset-1"><h1 >My Little Projects</h1></div>            <div class="row">                  <% @project.each do |project|%>                              <div class="col-md-10 col-md-offset-1 app">                                    <h1><%= project.title%></h1>                                    <hr>                                    <p><%= project.description%> </p>                                    <p><%= link_to project.link do %>                                              <i class="fa fa-github fa-lg"> Github Link</i>                                          <%end%>                                    </p>                                    <!-- После создания юзера, обрамить эти кнопки с доступом только юзеру-->                                    <%= link_to 'Edit', edit_project_path(project)  %>                                    |                                    <%= link_to 'Delete', project_path(project), method: :delete, data: { confirm: 'Are you sure?'} %>                              </div>                  <%end%>            </div>            <br/><br/><br/>      </div></div><hr>