<%= form_for @project do |f|%>      <% if @project.errors.any?%>      <h2><%= pluralize(@project.errors.count, 'errors')%> prevented this project from saving</h2>        <ul>              <% @project.errors.full_messages.each do |msg|%>                  <li><%= msg%></li>              <%end%>        </ul>      <%end%>    <%= f.text_field :title, placeholder:'Title'%>    <%= f.text_field :link,  placeholder:'Link'%>    <%= f.text_area :description, placeholder:'Description'%>    <%= f.submit 'SAVE', id:'submit'%><%end%>