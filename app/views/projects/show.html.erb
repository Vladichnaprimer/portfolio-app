<div class="row">      <div class="col-md-6 col-md-offset-2">            <h1><%= @project.title%></h1>            <hr>            <p><%=  @project.description%> </p>            <p><%= link_to @project.link do %>                      <i class="fa fa-github fa-lg"></i>                <%end%>            </p>            <br>            <%= link_to 'Edit', edit_project_path(@project)  %>            |            <%= link_to 'Delete', project_path(@project), method: :delete, data: { confirm: 'Are you sure?'} %>            <br><br>      </div></div>